<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<DialogPane maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="300.0"
            prefWidth="480.0"
            xmlns="http://javafx.com/javafx/8.0.76-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.bpcs.javafx.GroovyController">
    <header>
      <AnchorPane prefHeight="161.0" prefWidth="478.0">
         <children>
            <Label layoutX="44.0" layoutY="55.0" text="Enter User ID:" />
            <TextField fx:id="fldUserId" layoutX="159.0" layoutY="50.0" promptText="User ID" />
            <Label layoutX="44.0" layoutY="109.0" text="Enter Password:" />
            <PasswordField fx:id="fldPassword" layoutX="159.0" layoutY="106.0" promptText="Password" />
         </children></AnchorPane>
    </header>
    <content>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="320.0">
         <children>
            <Button fx:id="btnOk" layoutX="390.0" layoutY="9.0" mnemonicParsing="false" onAction="#onOkClicked" prefWidth="61.0" text="OK" />
            <Button fx:id="btnCancel" layoutX="314.0" layoutY="9.0" mnemonicParsing="false" onAction="#onCancelClicked" text="Cancel" />
            <Label fx:id="lblMessage" layoutX="314.0" layoutY="42.0" />
         </children></AnchorPane>
    </content>
    <expandableContent>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="320.0" />
    </expandableContent>
</DialogPane>
